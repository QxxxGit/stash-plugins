{
  "name": "qx-scene-page",
  "version": "1.0.0",
  "main": "qxScenePage.tsx",
  "license": "MIT",
  "scripts": {
    "build": "esbuild src/qxScenePage.tsx --bundle --outfile=dist/qxScenePage.js && yarn run sass && yarn run copy",
    "copy": "yarn run copy:yml && yarn run copy:fonts",
    "copy:yml": "cpx \"src/qxScenePage.yml\" \"dist\"",
    "copy:fonts": "cpx \"src/fonts/*\" \"dist/fonts/\"",
    "sass": "sass --no-source-map --style=expanded src/qxScenePage.scss:dist/qxScenePage.css"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "cpx": "^1.5.0",
    "esbuild": "^0.20.0",
    "sass": "^1.71.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
